# OpenAI API Key (required for LangGraph agents)
OPENAI_API_KEY=sk-your-openai-api-key-here

# LLM Configuration
LLM_MODEL=gpt-5.2
LLM_TEMPERATURE=0.0

# Reasoning Model Configuration (for o1, o3, gpt-5 models)
REASONING_ENABLED=true
REASONING_EFFORT=medium
REASONING_SUMMARY=auto

# Mistral AI Configuration (for PDF OCR)
MISTRAL_API_KEY=your-mistral-api-key-here

# Database Configuration
MYSQL_HOST=mysql
MYSQL_DATABASE=bbf_db
MYSQL_USER=admin
MYSQL_PASSWORD=admin
MYSQL_ROOT_PASSWORD=root

# Message Queue Configuration
BROKER_URL=redis://redis:6379/0

# Gunicorn/Worker Configuration
GUNICORN_CONCURRENCY=2
WORKER_CONCURRENCY=4

# Pod Type (web or worker)
POD_TYPE=web

# Phoenix Observability
PHOENIX_ENABLED=true
PHOENIX_COLLECTOR_ENDPOINT=http://phoenix:4317
PHOENIX_PROJECT_NAME=binary-brute-force-team
